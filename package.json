{
  "name": "rollup-plugin-multi-input",
  "version": "1.4.0",
  "description": "rollup plugin for bundling modular libraries",
  "type": "commonjs",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "esnext": "dist/plugin.mjs",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/plugin.ts",
    "build": "swc src/plugin.ts -o dist/plugin.mjs && swc src/plugin.ts -o dist/plugin.js module.type=commonjs",
    "prepare": "npm run lint && npm run build",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage",
    "test:ts": "tsc types/index.d.ts test/types.ts --noEmit"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "rollup-plugin",
    "rollup",
    "es6",
    "glob",
    "multi-entry",
    "modular",
    "libraries"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alfredosalzillo/rollup-plugin-multi-input.git"
  },
  "author": "alfredo salzillo <alfredo.salzillo@virgilio.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alfredosalzillo/rollup-plugin-multi-input/issues"
  },
  "homepage": "https://github.com/alfredosalzillo/rollup-plugin-multi-input#readme",
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@rollup/plugin-json": "^4.0.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.35",
    "@swc/jest": "^0.1.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^24.0.0",
    "jest": "^26.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "core-js": "^3.1.3",
    "fast-glob": "^3.0.0",
    "lodash": "^4.17.11"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
